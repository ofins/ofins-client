{
  "name": "@ofins/client",
  "version": "0.5.0",
  "description": "A package that provides useful utilities for frontend web applications",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/types/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./react": {
      "types": "./dist/types/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:watch": "npm run build -- --watch",
    "bump:minor": "npm version minor",
    "bump:patch": "npm version patch",
    "version:minor-alpha": "npm version preminor --preid=alpha --no-git-tag-version",
    "prepublishOnly": "npm run build",
    "lint": "npm run type:check && npm run prettier:check && npm run lint:code",
    "lint:fix": "npm run prettier:fix && npm run lint:code -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.4",
    "@types/react": "^19.1.10",
    "eslint": "^9.30.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rollup": "^4.48.0",
    "typescript": "^5.4.4",
    "typescript-eslint": "^8.36.0"
  },
  "directories": {
    "src": "src"
  }
}
