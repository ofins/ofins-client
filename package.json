{
  "name": "@ofins/client",
  "version": "0.2.0-alpha.0",
  "description": "A package that provides useful utilities for frontend web applications",
  "type": "module",
  "sideEffects": false,
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/lib/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:watch": "npm run build -- --watch",
    "bump:minor": "npm version minor",
    "bump:patch": "npm version patch",
    "version:minor-alpha": "npm version preminor --preid=alpha --no-git-tag-version",
    "prepublishOnly": "npm run build",
    "publish": "npm publish --access public",
    "lint": "npm run type:check && npm run prettier:check && npm run lint:code",
    "lint:fix": "npm run prettier:fix && npm run lint:code -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.4",
    "@types/react": "^19.1.10",
    "esbuild": "^0.25.9",
    "eslint": "^9.30.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^8.36.0"
  },
  "directories": {
    "lib": "lib"
  }
}
